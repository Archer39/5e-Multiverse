"midi-qol\": {\n            \"effectActivation\": false,\n            \"onUseMacroName\": \"[postActiveEffects]ItemMacro\"\n        },\n        \"itemacro\": {\n            \"macro\": {\n                \"name\": \"Forma selvatica (Origine)\",\n                \"type\": \"script\",\n                \"author\": \"Tyd5yiqWrRZMvG30\",\n                \"img\": \"icons/svg/dice-target.svg\",\n                \"scope\": \"global\",\n                \"command\": \"async function wait(ms) { return new Promise(resolve => { setTimeout(resolve, ms); }); }\\nconst lastArg = args[args.length - 1];\\nconst version = Math.floor(game.version);\\nconst fileSystem = version > 9 ? \\\"system\\\" : \\\"data\\\";\\nlet sequencerCheck = ((game.modules.get(\\\"sequencer\\\")) && (game.modules.get(\\\"sequencer\\\")?.active)) ? true : false;\\nlet jb2aCheck = ((game.modules.get(\\\"JB2A_DnD5e\\\")) && (game.modules.get(\\\"JB2A_DnD5e\\\")?.active)) || ((game.modules.get(\\\"jb2a_patreon\\\")) && (game.modules.get(\\\"jb2a_patreon\\\")?.active)) ? true : false;\\nlet tactor;\\nif (lastArg.tokenId) tactor = canvas.tokens.get(lastArg.tokenId).actor;\\nelse tactor = game.actors.get(lastArg.actorId);\\nconst tactorData = await tactor.getRollData();\\nlet target = canvas.tokens.get(lastArg.tokenId);\\nlet getResources = tactorData.resources.tertiary.value;\\nlet getEffect = tactor.effects.find(i => (version > 9 ? i.label : i.data.label) === \\\"Wild Shape\\\");\\nlet effectActor = await game.actors.get(version > 9 ? getEffect.changes[0].value : getEffect.data.changes[0].value);\\nlet keys = Object.keys(tactorData.spells);\\nlet updates = keys.reduce((acc, values, i) => {\\n    let primaryLabel = `${fileSystem}.resources.primary.label`;\\n    let primaryValue = `${fileSystem}.resources.primary.value`;\\n    let primaryMax = `${fileSystem}.resources.primary.max`;\\n    let secondaryLabel = `${fileSystem}.resources.secondary.label`;\\n    let secondaryValue = `${fileSystem}.resources.secondary.value`;\\n    let secondaryMax = `${fileSystem}.resources.secondary.max`;\\n    let tertiaryLabel = `${fileSystem}.resources.tertiary.label`;\\n    let tertiaryValue = `${fileSystem}.resources.tertiary.value`;\\n    let tertiaryMax = `${fileSystem}.resources.tertiary.max`;\\n    let spellMin = `${fileSystem}.spells.${values}.value`;\\n    let spellMax = `${fileSystem}.spells.${values}.max`;\\n    tactorData.resources.primary.label != null ? acc[primaryLabel] = tactorData.resources.primary.label : null;\\n    tactorData.resources.primary.label != null ? acc[primaryValue] = tactorData.resources.primary.value : null;\\n    tactorData.resources.primary.label != null ? acc[primaryMax] = tactorData.resources.primary.max : null;\\n    tactorData.resources.secondary.label != null ? acc[secondaryLabel] = tactorData?.resources.secondary.label : null;\\n    tactorData.resources.secondary.label != null ? acc[secondaryValue] = tactorData?.resources.secondary.value : null;\\n    tactorData.resources.secondary.label != null ? acc[secondaryMax] = tactorData.resources.secondary.max : null;\\n    tactorData.resources.tertiary.label != null ? acc[tertiaryLabel] = tactorData.resources.tertiary.label : null;\\n    tactorData.resources.tertiary.label != null ? acc[tertiaryValue] = tactorData.resources.tertiary.value : null;\\n    tactorData.resources.tertiary.label != null ? acc[tertiaryMax] = tactorData.resources.tertiary.max : null;\\n    acc[spellMin] = Object.values(tactorData.spells)[i].value;\\n    acc[spellMax] = Object.values(tactorData.spells)[i].max;\\n    return acc;\\n}, {});\\nawait effectActor.update(updates);\\nawait wait(500);\\nconst getToken = duplicate(version > 9 ? target.document : target.document.data);\\nif ((!sequencerCheck) && (!jb2aCheck)){\\n    await tactor.revertOriginalForm();    \\n    await effectActor.revertOriginalForm();        \\n    await MidiQOL.socket().executeAsGM(\\\"removeEffects\\\", { actorUuid: effectActor.uuid, effects: [getEffect.id] });\\n    await canvas.scene.updateEmbeddedDocuments(\\\"Token\\\", [{\\\"_id\\\": getToken._id, \\\"displayBars\\\" : CONST.TOKEN_DISPLAY_MODES.ALWAYS, \\\"mirrorX\\\" : getToken.mirrorX, \\\"mirrorY\\\" : getToken.mirrorY, \\\"rotation\\\" : getToken.rotation, \\\"elevation\\\": getToken.elevation}]);\\n    await game.actors.get(tactor.id)?.delete();\\n} else {\\n    new Sequence()\\n        .effect()\\n        .atLocation(target)\\n        .file(\\\"jb2a.misty_step.02.green\\\")\\n        .scaleToObject(1.5)\\n        .wait(1000)\\n        .thenDo(async function(){            \\n            await tactor.revertOriginalForm();            \\n            await effectActor.revertOriginalForm();\\n            await MidiQOL.socket().executeAsGM(\\\"removeEffects\\\", { actorUuid: effectActor.uuid, effects: [getEffect.id] });                       \\n            await canvas.scene.updateEmbeddedDocuments(\\\"Token\\\", [{\\\"_id\\\": getToken._id, \\\"displayBars\\\" : CONST.TOKEN_DISPLAY_MODES.ALWAYS, \\\"mirrorX\\\" : getToken.mirrorX, \\\"mirrorY\\\" : getToken.mirrorY, \\\"rotation\\\" : getToken.rotation, \\\"elevation\\\": getToken.elevation}]);\\n            await game.actors.get(tactor.id)?.delete();\\n        })\\n    .play()\\n}\"\n            }\n        }\n    }\n}];\n\nif ((args[0] === \"on\") && (!tactor.isPolymorphed)) {\n    await new Dialog({\n        title: itemD.name,\n        content: the_content,\n        buttons: {\n            change: {\n                label: \"Change\", callback: async (html) => {\n                    let polyId = html.find('#beast')[0].value;\n                    let checkEquipped = html.find('#equipped:checked')[0]?.value;\n                    let transferEquipment = checkEquipped === \"on\" ? true : false;\n                    let findToken = getFolder.find(i => i.id === polyId);\n                    const getToken = duplicate(version > 9 ? target.document : target.document.data);\n                    let switches = { keepBio: true, keepClass: true, keepMental: true, mergeSaves: true, keepFeats: true, mergeSkills: true, transformTokens: true, keepAE: true };\n                    switches[\"keepItems\"] = transferEquipment;\n                    switches[\"keepEquipmentAE\"] = transferEquipment;\n                    if ((!sequencerCheck) && (!jb2aCheck)) {\n                        await tactor.transformInto(findToken, switches);\n                    } else {\n                        new Sequence()\n                            .effect()\n                            .atLocation(target)\n                            .file(\"jb2a.misty_step.01.green\")\n                            .scaleToObject(1.5)\n                            .thenDo(async function () {\n                                await tactor.transformInto(findToken, switches);\n                            })\n                            .play()\n                    }\n                    await wait(1000);\n                    let combatWildShape = tactor.items.find(i => i.name === \"Combat Wild Shape\");\n                    let findPoly = await game.actors.find(i => i.name === `${tactor.name} (${findToken.name})`);\n                    if(!findPoly)\n                    await canvas.scene.updateEmbeddedDocuments(\"Token\", [{ \"_id\": getToken._id, \"displayBars\": CONST.TOKEN_DISPLAY_MODES.ALWAYS, \"mirrorX\": getToken.mirrorX, \"mirrorY\": getToken.mirrorY, \"rotation\": getToken.rotation, \"elevation\": getToken.elevation }]);\n                    let keys = Object.keys(tactorData.spells);\n                    let updates = keys.reduce((acc, values, i) => {\n                        let primaryLabel = `${fileSystem}.resources.primary.label`;\n                        let primaryValue = `${fileSystem}.resources.primary.value`;\n                        let primaryMax = `${fileSystem}.resources.primary.max`;\n                        let secondaryLabel = `${fileSystem}.resources.secondary.label`;\n                        let secondaryValue = `${fileSystem}.resources.secondary.value`;\n                        let secondaryMax = `${fileSystem}.resources.secondary.max`;\n                        let tertiaryLabel = `${fileSystem}.resources.tertiary.label`;\n                        let tertiaryValue = `${fileSystem}.resources.tertiary.value`;\n                        let tertiaryMax = `${fileSystem}.resources.tertiary.max`;\n                        let spellMin = `${fileSystem}.spells.${values}.value`;\n                        let spellMax = `${fileSystem}.spells.${values}.max`;\n                        if (combatWildShape) {\n                            acc[spellMin] = Object.values(tactorData.spells)[i].value;\n                            acc[spellMax] = Object.values(tactorData.spells)[i].max;\n                        }\n                        if (tactorData.resources.primary.label != null) {\n                            acc[primaryLabel] = tactorData.resources.primary.label;\n                            acc[primaryValue] = tactorData.resources.primary.value;\n                            acc[primaryMax] = tactorData.resources.primary.max;\n                        }\n                        if (tactorData.resources.secondary.label != null) {\n                            acc[secondaryLabel] = tactorData.resources.secondary.label;\n                            acc[secondaryValue] = tactorData.resources.secondary.value;\n                            acc[secondaryMax] = tactorData.resources.secondary.max;\n                        }\n                        if (tactorData.resources.tertiary.label != null) {\n                            acc[tertiaryLabel] = tactorData.resources.tertiary.label;\n                            acc[tertiaryValue] = tactorData.resources.tertiary.value;\n                            acc[tertiaryMax] = tactorData.resources.tertiary.max;\n                        }\n                        return acc;\n                    }, {});\n                    await findPoly.update(updates);                    \n                    if (combatWildShape){\n                        wildshapeRevert.push(wildshapeHealing);\n                        await ui.notifications.warn(`Aggiunta Guarigione della Forma selvatica: Usala per curarti.`);\n                    }\n                    await findPoly.createEmbeddedDocuments(\"Item\", wildshapeRevert);\n                    await ui.notifications.warn(`Aggiunta Forma selvatica(Origine): Usala per ritornare come prima.`);                    \n                }\n            }\n        },\n        default: \"change\"\n    }).render(true);\n}\nif ((args[0] === \"on\") && (tactor.isPolymorphed)) {\n    let effect = tactor.effects.find(i => (version > 9 ? i.label : i.data.label) === itemD.name);\n    if (effect) await MidiQOL.socket().executeAsGM(\"removeEffects\", { actorUuid: tactor.uuid, effects: [effect.id] });\n}\nif (args[0] === \"off\") {\n    await tactor.revertOriginalForm();\n}","_id":null,"folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"core":{"sourceId":"Item.R2Pf3RpLEhvLi4rI"},"cf":{"id":"temp_1uaqzdbkxui"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1677161749541,"modifiedTime":1689963793410,"lastModifiedBy":"ofPRaS27mgPnSNVL"},"folder":null,"sort":0,"_id":"GaiWPirAyz0stzz1"}
{"name":"Competenze bonus","type":"feat","system":{"description":{"value":"<p>Quando scegli questo archetipo al 3° livello, ottieni la competenza con il kit del travestimento e il kit dell'avvelenatore.</p>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":null,"condition":""},"duration":{"value":"","units":""},"cover":null,"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":"","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"type":{"value":"","subtype":""},"requirements":"Assassino 3","recharge":{"value":null,"charged":false}},"img":"icons/skills/toxins/symbol-poison-drop-skull-green.webp","effects":[{"label":"Competenze bonus","icon":"icons/skills/toxins/symbol-poison-drop-skull-green.webp","origin":"Actor.ckuXxz6aO8UGJeyv.Item.wzT3XEde2eghTScl","duration":{"startTime":null,"seconds":null,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"disabled":false,"_id":"fFHJJpbbANVtFjLs","changes":[{"key":"system.traits.toolProf.value","mode":2,"value":"disg","priority":20},{"key":"system.traits.toolProf.value","mode":2,"value":"pois","priority":20}],"tint":null,"transfer":true,"flags":{"dae":{"selfTarget":false,"selfTargetAlways":false,"stackable":"noneName","durationExpression":"","macroRepeat":"none","specialDuration":[]},"core":{"statusId":""},"ActiveAuras":{"isAura":false,"aura":"None","radius":"undefined","alignment":"","type":"","ignoreSelf":false,"height":false,"hidden":false,"displayTemp":false,"hostile":false,"onlyOnce":false}}}],"ownership":{"default":0,"elfWiUwSIhZ4c7AC":3,"3gqesFFC9fBUrWgj":3},"flags":{"magicitems":{"enabled":false,"equipped":false,"attuned":false,"charges":"0","chargeType":"c1","destroy":false,"destroyFlavorText":"ha esaurito le cariche: si sbriciola in cenere e viene distrutto.","rechargeable":false,"recharge":"0","rechargeType":"t1","rechargeUnit":"r1","sorting":"l"},"core":{"sourceId":"Actor.ckuXxz6aO8UGJeyv.Item.wzT3XEde2eghTScl"},"cf":{"id":"temp_mkjjxeld5jo"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1690410302579,"modifiedTime":1690419504027,"lastModifiedBy":"3gqesFFC9fBUrWgj"},"folder":null,"sort":0,"_id":"Gc8stW0d0PN6J3V7"}
{"name":"Spadaccino","type":"subclass","img":"icons/skills/melee/maneuver-sword-katana-yellow.webp","system":{"description":{"value":"<p>Il ladro ha incentrato il suo addestramento sull'arte della lama, affidandosi alla velocità, all'eleganza e al fascino in egual misura. Altri combattenti possono essere dei bruti protetti da pesanti armature, ma il metodo di combattimento di questo ladro sembra quasi un'esibizione. I duellanti e i pirati solitamente appartengono a questo archetipo. Uno Spadaccino eccelle negli scontri singoli e può combattere con due armi e guizzare oltre la portata di un avversario senza correre rischi.</p><hr /><h3><span style=\"color:#9F000F\"><strong>@UUID[Compendium.multiverse5e.sottoclassi-privilegi.iUeUCBLndBnXNHeC]{Gioco di Gambe}</strong></span></h3><p>Quando il ladro sceglie questo archetipo al 3° livello, impara a sferrare un colpo e ad allontanarsi senza temere rappresaglie. Durante il suo turno, se il ladro effettua un attacco in mischia contro una creatura, quella creatura non può effettuare attacchi di opportunità contro di lui per il resto del turno del ladro.</p><h3><span style=\"color:#9F000F\"><strong>@UUID[Compendium.multiverse5e.sottoclassi-privilegi.9nVk5n4MYnQgoC0S]{Audacia del Fanfarone}</strong></span></h3><p>A partire dal 3° livello, il ladro si lascia guidare in battaglia dalla sua inconfondibile sicurezza. Il ladro può aggiungere il suo modificatore di Carisma ai suoi tiri per l'iniziativa.</p><p>Inoltre, il ladro non ha bisogno di vantaggio al suo tiro per colpire per usare il suo Attacco Furtivo se nessuna creatura oltre al suo bersaglio si trova entro 1,5 metri da lui e se non subisce svantaggio al tiro per colpire. Tutte le altre regole relative al privilegio di classe Attacco Furtivo si applicano comunque al ladro.</p><blockquote><p><code>Sage Advice Compendium: Ora lo Spadaccino non deve disporre di svantaggio al tiro per colpire per poter usare questo privilegio</code></p></blockquote><h3><span style=\"color:#9F000F\"><strong>@UUID[Compendium.multiverse5e.sottoclassi-privilegi.6iVMhmKZberMeBuL]{Spavalderia}</strong></span></h3><p>Al 9° livello, il fascino del ladro diventa ammaliante e irresistibile. Con un'azione, il ladro può effettuare una prova di Carisma (Persuasione) contrapposta a una prova di Saggezza (Intuizione) di una creatura. La creatura deve essere in grado di sentire il ladro e i due devono condividere un linguaggio.</p><p>Se il ladro supera la prova e la creatura gli è ostile, essa subisce svantaggio ai tiri per colpire contro i bersagli diversi dal ladro e non può effettuare attacchi di opportunità contro nessun bersaglio diverso da lui. Questo effetto dura per 1 minuto, finché un compagno del ladro non attacca il bersaglio o non lo influenza con un incantesimo, o finché il ladro e il bersaglio non si trovano a più di 18 metri di distanza.</p><p>Se il ladro supera la prova e la creatura non gli è ostile, essa è affascinata dal ladro per 1 minuto. Finché la creatura è affascinata, considera il ladro una conoscenza amichevole. Questo effetto termina immediatamente se il ladro o i suoi compagni fanno qualcosa di dannoso nei confronti della creatura.</p><h3><span style=\"color:#9F000F\"><strong>@UUID[Compendium.multiverse5e.sottoclassi-privilegi.5jsggLGsw1znPoiK]{Manovra Elegante}</strong></span></h3><p>A partire dal 13° livello, il ladro può usare un'azione bonus nel suo turno per disporre di vantaggio alla successiva prova di Destrezza (Acrobazia) o Forza (Atletica) che il ladro effettua durante lo stesso turno.</p><h3><span style=\"color:#9F000F\"><strong>@UUID[Compendium.multiverse5e.sottoclassi-privilegi.qgxUqnlyjEWATC6m]{Maestro Duellante}</strong></span></h3><p>A partire dal 17° livello, grazie alla sua maestria nell'uso della lama, il ladro può trasformare un fallimento in successo durante un combattimento. Se il ladro manca con un tiro per colpire, può ripetere il tiro disponendo di vantaggio. Quando lo fa, non può usare di nuovo questo privilegio finché non completa un riposo lungo o breve.</p>","chat":"","unidentified":""},"source":"SCAG/XGtE","identifier":"spadaccino","classIdentifier":"ladro","advancement":[{"_id":"96gOVuU6HKZnT2dG","type":"ItemGrant","configuration":{"items":["Compendium.multiverse5e.sottoclassi-privilegi.iUeUCBLndBnXNHeC","Compendium.multiverse5e.sottoclassi-privilegi.9nVk5n4MYnQgoC0S"],"optional":false,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{},"level":3,"title":""},{"_id":"zTMCuNiZFQNuBWru","type":"ItemGrant","configuration":{"items":["Compendium.multiverse5e.sottoclassi-privilegi.6iVMhmKZberMeBuL"],"optional":false,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{},"level":9,"title":""},{"_id":"XxvPJfErTnOnkPCU","type":"ItemGrant","configuration":{"items":["Compendium.multiverse5e.sottoclassi-privilegi.5jsggLGsw1znPoiK"],"optional":false,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{},"level":13,"title":""},{"_id":"ca4uZKm51EGYy1a7","type":"ItemGrant","configuration":{"items":["Compendium.multiverse5e.sottoclassi-privilegi.qgxUqnlyjEWATC6m"],"optional":false,"spell":{"ability":"","preparation":"","uses":{"max":"","per":""}}},"value":{},"level":17,"title":""}],"spellcasting":{"progression":"none","ability":""}},"effects":[],"ownership":{"default":0,"8nuzI5cCRWxggG8F":3},"flags":{"core":{"sourceId":"Item.vjASUC8n7hTF37SF"},"cf":{"id":"temp_rud7ik8ai2"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.1.5","coreVersion":"10.291","createdTime":1688734879011,"modifiedTime":1688736266548,"lastModifiedBy":"8nuzI5cCRWxggG8F"},"folder":null,"sort":0,"_id":"GmNmPcjVIGtWFCh6"}
{"name":"Favored Foe","type":"spell","img":"icons/skills/targeting/crosshair-pointed-orange.webp","system":{"description":{"value":"<p data-content-chunk-id=\"4a3e5f32-acb8-43db-818b-37d1302e8fa8\">When you hit a creature with an attack roll, you can call on your mystical bond with nature to mark the target as your favored enemy for 1 minute or until you lose your concentration (as if you were concentrating on a spell).</p>\n<p data-content-chunk-id=\"7db745a2-977a-4432-a38e-91d1a26760b2\">The first time on each of your turns that you hit the favored enemy and deal damage to it, including when you mark it, you can increase that damage by 1d4.</p>\n<p data-content-chunk-id=\"3bfda3fe-fdd9-4c20-b20c-5d456ca1cbb0\">You can use this feature to mark a favored enemy a number of times equal to your proficiency bonus, and you regain all expended uses when you finish a long rest.</p>\n<p data-content-chunk-id=\"fcb93e19-8eb5-4200-ba15-f27ace0dc962\">This feature’s extra damage increases when you reach certain levels in this class: to 1d6 at 6th level and to 1d8 at 14th level.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"special","cost":null,"condition":""},"duration":{"value":1,"units":"minute"},"target":{"value":1,"width":null,"units":"","type":"creature"},"range":{"value":60,"long":null,"units":"ft"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"attribute","target":"","amount":1},"ability":"","actionType":"util","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":null},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"level":0,"school":"enc","components":{"value":"","vocal":false,"somatic":false,"material":false,"ritual":false,"concentration":true},"materials":{"value":"","consumed":false,"cost":0,"supply":0},"preparation":{"mode":"atwill","prepared":true},"scaling":{"mode":"none","formula":""}},"effects":[{"_id":"QeFbXAbsgp1AYAGs","changes":[],"disabled":false,"duration":{"startTime":null,"seconds":3600,"combat":null,"rounds":null,"turns":null,"startRound":null,"startTurn":null},"icon":"icons/skills/targeting/crosshair-pointed-orange.webp","label":"Favored Foe","transfer":false,"flags":{"dae":{"stackable":"none","macroRepeat":"none","specialDuration":["zeroHP"],"transfer":false,"selfTarget":false,"durationExpression":""},"core":{"statusId":""},"dnd5e-helpers":{"rest-effect":"Ignore"},"ActiveAuras":{"isAura":false,"aura":"None","radius":null,"alignment":"","type":"","ignoreSelf":false,"height":false,"hidden":false,"displayTemp":false,"hostile":false,"onlyOnce":false}},"tint":null,"origin":null}],"flags":{"itemacro":{"macro":{"data":{"_id":null,"name":"Favored Foe","type":"script","author":"Tyd5yiqWrRZMvG30","img":"icons/svg/dice-target.svg","scope":"global","command":"////////////////////////////////////////////////\n// #READ FIRST\n// # Remade version of Hunters Mark by Tposney\n// # Requires Midi-qol On Use\n// # Create Item as a Cantrip At-will for concentration ease\n///////////////////////////////////////////////\nasync function wait(ms) { return new Promise(resolve => { setTimeout(resolve, ms); }); }\nconst lastArg = args[args.length - 1];\nconst tokenD = canvas.tokens.get(lastArg.tokenId);\nconst actorD = tokenD.actor;\nconst actorData = actorD.getRollData();\nconst classLevel = actorData.details.cr ?? actorData.classes.ranger.levels;\nconst itemD = lastArg.item;\nlet targetUuid;\nlet effectData;\n\nif (args[0].targets.length === 0) return {};\nif (args[0].tag === \"OnUse\") {\n    targetUuid = lastArg.targets[0].uuid;\n    let tactortUuid = lastArg.targets[0].actor.uuid;\n    if (!targetUuid) return console.error(\"Favored Foe: no token/target selected\");\n    let conc = actorD.effects.find(i => i.data.label === \"Concentrating\");\n    if (conc) {\n        let concUpdate = await getProperty(actorD.data.flags, \"midi-qol.concentration-data.targets\");\n        await concUpdate.push({ tokenUuid: targetUuid, actorUuid: tactortUuid });\n        await actorD.setFlag(\"midi-qol\", \"concentration-data.targets\", concUpdate);\n    }\n    effectData = {\n        changes: [\n            { key: \"flags.midi-qol.favoredFoe\", mode: CONST.ACTIVE_EFFECT_MODES.OVERRIDE, value: targetUuid, priority: 20 },\n            { key: \"flags.dae.deleteUuid\", mode: CONST.ACTIVE_EFFECT_MODES.OVERRIDE, value: conc.uuid, priority: 20 },\n            { key: \"flags.dnd5e.DamageBonusMacro\", mode: CONST.ACTIVE_EFFECT_MODES.CUSTOM, value: `ItemMacro.${itemD.name}`, priority: 20 }\n        ],\n        origin: lastArg.itemUuid,\n        disabled: false,\n        flags: { dae: { stackable: \"noneOrigin\", tokenId: tokenD.id, itemData: itemD } },\n        duration: duplicate(itemD.effects[0].duration),\n        icon: itemD.img,\n        label: itemD.name\n    }\n    effectData.duration.startTime = game.time.worldTime;\n    await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: actorD.uuid, effects: [effectData] });\n    await wait(1000);\n    let target = await canvas.tokens.get(lastArg.targets[0].id);\n    let effect = target.actor.effects.find(i => i.data.label === itemD.name);\n    if (effect) {\n        let updateData = {\n            _id: effect.id,\n            changes: [\n                { key: \"flags.dae.deleteUuid\", mode: CONST.ACTIVE_EFFECT_MODES.OVERRIDE, value: conc.uuid, priority: 20 }\n            ],\n            duration: duplicate(itemD.effects[0].duration)\n        }\n        await MidiQOL.socket().executeAsGM(\"updateEffects\", { actorUuid: target.actor.uuid, updates: [updateData] });\n    }\n}\n\nif (args[0].tag === \"DamageBonus\") {\n    if (![\"mwak\", \"rwak\"].includes(itemD.data.actionType)) return {};\n    targetUuid = lastArg.hitTargets[0].uuid;\n    let favoredFoe = actorD.items.getName(\"Favored Foe\");\n    if (targetUuid !== getProperty(lastArg.actor.data.flags, \"midi-qol.favoredFoe\")) return {};\n    if (actorD.effects.find(i => i.data.label === `${favoredFoe.name} Effect`)) return {};\n    let gameRound = game.combat ? game.combat.round : 0;\n    effectData = {\n        label: `${favoredFoe.name} Effect`,\n        icon: favoredFoe.img,\n        tint: \"#8f0000\",\n        origin: favoredFoe.uuid,\n        flags: { dae: { stackable: \"noneName\", tokenId: tokenD.id, itemData: itemD, specialDuration: [\"turnEndSource\", \"turnStartSource\"] } },\n        duration: { turns: 1, startRound: gameRound, startTime: game.time.worldTime }\n    };\n    await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: actorD.uuid, effects: [effectData] });\n    let damageType = lastArg.defaultDamageType ?? lastArg.damageDetail.map(i => i.type)[0];\n    let dieFace = classLevel >= 14 ? 8 : classLevel >= 6 ? 6 : 4;\n    let damangeDice = await new game.dnd5e.dice.DamageRoll(`1d${dieFace}[${damageType}]`, actorData, { critical: lastArg.isCritical }).evaluate({ async: true });\n    return { damageRoll: damangeDice.formula, flavor: `(${favoredFoe.name} (${CONFIG.DND5E.damageTypes[damageType]}))` };\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"core":{"sourceId":"Item.99mweYiHXGE9UvRz"},"midi-qol":{"onUseMacroName":"[postActiveEffects]ItemMacro","effectActivation":false},"favtab":{"isFavorite":true},"exportSource":{"world":"darklands","system":"dnd5e","coreVersion":"9.269","systemVersion":"1.6.3"},"enhanced-terrain-layer":{"multiple":1,"min":0,"max":0,"environment":""},"midiProperties":{"nodam":false,"fulldam":false,"halfdam":false,"rollOther":false,"critOther":false,"magicdam":false,"magiceffect":false,"concentration":false,"toggleEffect":false,"ignoreTotalCover":false},"cf":{"id":"temp_y4jley1gm8"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.291","createdTime":1669440918461,"modifiedTime":1674059135085,"lastModifiedBy":"ofPRaS27mgPnSNVL"},"_id":"GuDU3PvD2G6EpLKz","folder":null,"sort":0,"ownership":{"default":0,"T4cu5jv0VXcrhb1w":3}}
{"name":"Supplica: Catene di Carceri","type":"feat","img":"icons/sundries/books/book-purple-detail.webp","system":{"description":{"value":"<p><span style=\"font-family:Signika, sans-serif\"><em style=\"box-sizing:border-box;user-select:text\">Prerequisiti: 15° livello, privile</em></span>gio Patto della Catena</p><p>Il warlock può lanciare Blocca Mostri a vo<span style=\"font-family:Signika, sans-serif\">lontà (bersagliando un celestiale, un immondo o un elementale) senza spendere uno slot incantesimo o componenti materiali. Deve completare un riposo lungo prima che possa usare di nuovo questa supplica sulla stessa creatura. </span></p>","chat":"","unidentified":""},"source":"","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":null,"recovery":""},"consume":{"type":"","target":null,"amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":null,"charged":false}},"effects":[],"ownership":{"default":0,"cerf0tYYbyMdOK0R":3,"vKzz5KHp2wQguism":3},"flags":{"core":{"sourceId":"Item.Ce9qz2EBaUpSIfE1"},"cf":{"id":"temp_ht01j84ne58"}},"_stats":{"systemId":"dnd5e","systemVersion":"2.0.3","coreVersion":"10.290","createdTime":1669727933212,"modifiedTime":1669738792791,"lastModifiedBy":"cerf0tYYbyMdOK0R"},"folder":null,"sort":0,"_id":"H4dpgj4LMkjFv0GS"}
