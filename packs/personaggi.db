ed = game.modules.get(\"warpgate\") && !game.modules.get(\"warpgate\").active ? \"enabled\" : \"installed\";\n    error = `You need to have WarpGate ${installed} to run this macro!`;\n}\n\nconst lastArg = args[args.length - 1];\nconst target = canvas.tokens.get(lastArg.tokenId);\nconst gridsize = await canvas.grid.size/2;\nlet cachedDistance = 0;\nlet cachedPosition = {x: 0, y: 0};\nconst itemList = [\"Fuming\", \"Mirthful\", \"Tricksy\"];\nconst getItem = await target.actor.itemTypes.feat.filter(i=> itemList.includes(i.name))[0];\n\n\nlet range = await canvas.scene.createEmbeddedDocuments(\"MeasuredTemplate\", [{\n    t: \"circle\",\n    user: game.user.id,\n    x: target.x + canvas.grid.size / 2,\n    y: target.y + canvas.grid.size / 2,\n    direction: 0,\n    distance: 30,\n    borderColor: \"#FF0000\"\n}]);\n\nlet position = await warpgate.crosshairs.show({\n    size: 1,\n    tag: randomID(),\n    label: \"Teleport to 0 ft\",    \n    drawOutline: false,\n    drawIcon: false\n}, { show: async (crosshair) => {\n    await target.actor.sheet.minimize(true);\n    new Sequence()\n        .effect()\n            .from(target)\n            .attachTo(crosshair)\n            .persist()\n            .opacity(0.5)\n        .play();\n    cachedPosition.x = crosshair.x;\n    cachedPosition.y = crosshair.y;    \n\n    while (crosshair.inFlight) {\n        \n        //wait for initial render\n        await warpgate.wait(100);\n        if ((cachedPosition.x !== crosshair.x) || (cachedPosition.y !== crosshair.y)) {\n          \n            cachedPosition.x = crosshair.x;\n            cachedPosition.y = crosshair.y;             \n  \n            const ray = new Ray( target.center, crosshair.center );\n            cachedDistance = Number(canvas.grid.measureDistances([{ray}], {gridSpaces:true})[0]); \n\n            crosshair.draw();\n            \n            crosshair.label = `Teleport to ${cachedDistance} ft`;            \n          }\n    }\n}})\n\nawait range[0].delete();\n\n\n\nnew Sequence()\n    .effect()\n        .file(\"jb2a.misty_step.01.dark_black\")\n        .atLocation(target)\n        .randomRotation()\n        .scale(target.data.width/2)\n    .wait(750)\n    .animation()\n        .on(target)\n        .opacity(0.0)\n        .waitUntilFinished()\n    .animation()\n        .on(target)\n        .teleportTo(position)\n        .snapToGrid()\n        .waitUntilFinished()\n    .effect()\n        .file(\"jb2a.misty_step.02.dark_black\")\n        .atLocation(target)\n        .randomRotation()\n        .scale(target.data.width/2)\n    .wait(1500)\n    .animation()\n        .on(target)\n        .opacity(1.0)\n    .thenDo(async function(){\n        await target.actor.sheet.maximize(true);\n        console.log(getItem);\n        await getItem.roll();\n    })\n    .play();","folder":null,"sort":0,"permission":{"default":0},"flags":{}},"_id":null,"name":"Fey Step","type":"script","author":"Tyd5yiqWrRZMvG30","img":"icons/svg/dice-target.svg","scope":"global","command":"// Requires the JB2A module - if you're using their free version, change the path to point to the right location\n// Then add this macro's name to the bottom of the Misty Step spell in the \"On Use Macro\" field\nconst lastArg = args[args.length - 1];\nconst version = Math.floor(game.version);\nconst target = canvas.tokens.get(lastArg.tokenId);\nconst gridsize = await canvas.grid.size / 2;\nlet cachedDistance = 0;\nlet cachedPosition = { x: 0, y: 0 };\nconst itemList = [\"Fuming\", \"Mirthful\", \"Tricksy\"];\nconst getItem = await target.actor.itemTypes.feat.filter(i => itemList.includes(i.name))[0];\n\nlet warpgateCheck = ((game.modules.get(\"warpgate\")) && (game.modules.get(\"warpgate\")?.active)) ? true : false;\nlet sequencerCheck = ((game.modules.get(\"sequencer\")) && (game.modules.get(\"sequencer\")?.active)) ? true : false;\nlet jb2aCheck = ((game.modules.get(\"JB2A_DnD5e\")) && (game.modules.get(\"JB2A_DnD5e\")?.active)) || ((game.modules.get(\"jb2a_patreon\")) && (game.modules.get(\"jb2a_patreon\")?.active)) ? true : false;\n\nif (!warpgateCheck || !sequencerCheck || !jb2aCheck) {\n    if (!warpgateCheck) ui.notifications.error(\"Warpgate Module is missing, Install or Enable it.\");\n    if (!sequencerCheck) ui.notifications.error(\"Sequencer Module is missing, Install or Enable it.\");\n    if (!jb2aCheck) ui.notifications.error(\"JB2A is required [free or paid version], install or Enable one of them\");\n    return;\n}\nlet teleportIn;\nlet teleportOut;\nif (((game.modules.get(\"JB2A_DnD5e\")) && (game.modules.get(\"JB2A_DnD5e\")?.active))) teleportIn = `jb2a.misty_step.01.blue`;\nelse if ((game.modules.get(\"jb2a_patreon\")) && (game.modules.get(\"jb2a_patreon\")?.active)) teleportIn = `jb2a.misty_step.01.dark_black`;\n\nif (((game.modules.get(\"JB2A_DnD5e\")) && (game.modules.get(\"JB2A_DnD5e\")?.active))) teleportOut = `jb2a.misty_step.02.blue`;\nelse if ((game.modules.get(\"jb2a_patreon\")) && (game.modules.get(\"jb2a_patreon\")?.active)) teleportOut = `jb2a.misty_step.02.dark_black`;\n\n\nlet range = await canvas.scene.createEmbeddedDocuments(\"MeasuredTemplate\", [{\n    t: \"circle\",\n    user: game.user.id,\n    x: target.x + canvas.grid.size / 2,\n    y: target.y + canvas.grid.size / 2,\n    direction: 0,\n    distance: 30,\n    borderColor: \"#FF0000\"\n}]);\n\nlet position = await warpgate.crosshairs.show({\n    size: 1,\n    tag: randomID(),\n    label: \"Teleport to 0 ft\",\n    drawOutline: false,\n    drawIcon: false\n}, {\n    show: async (crosshair) => {\n        await target.actor.sheet.minimize(true);\n        new Sequence()\n            .effect()\n            .from(target)\n            .attachTo(crosshair)\n            .persist()\n            .opacity(0.5)\n            .play();\n        cachedPosition.x = crosshair.x;\n        cachedPosition.y = crosshair.y;\n\n        while (crosshair.inFlight) {\n\n            //wait for initial render\n            await warpgate.wait(100);\n            if ((cachedPosition.x !== crosshair.x) || (cachedPosition.y !== crosshair.y)) {\n\n                cachedPosition.x = crosshair.x;\n                cachedPosition.y = crosshair.y;\n\n                const ray = new Ray(target.center, crosshair.center);\n                cachedDistance = Number(canvas.grid.measureDistances([{ ray }], { gridSpaces: true })[0]);\n\n                crosshair.draw();\n\n                crosshair.label = `Teleport to ${cachedDistance} ft`;\n            }\n        }\n    }\n})\n\nawait range[0].delete();\n\n\n\nnew Sequence()\n    .effect()\n    .file(teleportIn)\n    .atLocation(target)\n    .randomRotation()\n    .scale(0.5)\n    .wait(750)\n    .animation()\n    .on(target)\n    .opacity(0.0)\n    .waitUntilFinished()\n    .animation()\n    .on(target)\n    .teleportTo(position)\n    .snapToGrid()\n    .waitUntilFinished()\n    .effect()\n    .file(teleportOut)\n    .atLocation(target)\n    .randomRotation()\n    .scale(0.5)\n    .wait(1500)\n    .animation()\n    .on(target)\n    .opacity(1.0)\n    .thenDo(async function () {\n        //await target.actor.sheet.maximize(true);\n        if (getItem.name === \"Mirthful\") {\n            const targetList = await MidiQOL.findNearby(null, target, 10, null);\n            let targetFound = await new Promise((resolve) => {\n                let getTargets = targetList.reduce((list, item) => {\n                    if (target.id != item.id) list.push(`<label class=\"radio-label\"><input type=\"radio\" name=\"mirthTarget\" value=\"${item.id}\"><img src=\"${version > 9 ? item.document.texture : item.data.img}\"><div>${item.actor.name}</div></label>`);\n                    else list;\n                    return list;\n                }, []);\n                let content = `\n            <style>\n        #targetList .form-group {\n            display: flex;\n            flex-wrap: wrap;\n            width: 100%;\n            align-items: flex-start;\n          }\n          \n          #targetList .radio-label {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            text-align: center;\n            justify-items: center;\n            flex: 1 0 25%;\n            line-height: normal;\n          }\n          \n          #targetList .radio-label input {\n            display: none;\n            position:relative;\n          }\n    \n          #targetList .radio-label input div {\n           display:block;\n           text-align:center;\n           position: absolute;\n           bottom:5px;\n           left:0;\n           right:0;\n          }\n          \n          #targetList img {\n            border: 0px;\n            width: 50px;\n            height: 50px;\n            flex: 0 0 50px;\n            cursor: pointer;\n          }\n    \n          #targetList [type=radio]:hover + img {\n            outline: 2px solid #000;\n          }\n              \n          /* CHECKED STYLES */\n          #targetList [type=radio]:checked + img {\n            outline: 2px solid #f00;\n          }\n        </style>\n        <form id=\"targetList\">        \n        <div class=\"form-group\">Select a Target</div>\n            <div class=\"form-group\">${getTargets.join('')}</div></form>`;\n                new Dialog({\n                    title: getItem.name,\n                    content,\n                    buttons: {\n                        use: {\n                            label: `Select`, callback: async (html) => {\n                                let result = html.find(\"input[type='radio'][name='mirthTarget']:checked\");\n                                if (result.length === 0) {\n                                    resolve(false);\n                                    return ui.notifications.warn(`No target!`);\n                                }\n                                resolve(result[0].value);\n                            }\n                        },\n                        skip: {\n                            label: \"Skip\", callback: () => {\n                                return resolve(false);\n                            }\n                        }\n                    },\n                    default: \"Skip\"\n                }).render(true);\n            });\n            if (!targetFound) return {};\n            let finalTarget = await canvas.tokens.get(targetFound);\n            await finalTarget.setTarget(true, { releaseOthers: true });\n            version > 9 ? await getItem.use() : await getItem.roll();\n        } else {\n            version > 9 ? await getItem.use() : await getItem.roll();\n        }\n    })\n    .play();","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"system":{"description":{"value":"<p>The fey magically teleports up to 30 feet to an unoccupied space it can see. Then one of the following effects occurs, based on the fey’s chosen mood:</p>","chat":"","unidentified":""},"source":"","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value":null,"long":null,"units":"self"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"util","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"spell"},"requirements":"","recharge":{"value":null,"charged":false}},"ownership":{"default":0,"Tyd5yiqWrRZMvG30":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}},{"_id":"unJwYcR05C2VBP7n","name":"Fuming","type":"feat","img":"icons/skills/melee/sword-engraved-glow-purple.webp","effects":[{"_id":"txu4n6mxw6pzrud0","changes":[{"key":"flags.midi-qol.advantage.attack.all","mode":2,"value":"1","priority":20}],"disabled":false,"duration":{"startTime":null,"turns":1,"seconds":null,"comba