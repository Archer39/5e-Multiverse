e((resolve) => {\n        let getTargets = targetList.reduce((list, target) => {\n            if (casterId != target.id) list.push(`<label class=\"radio-label\"><input type=\"radio\" name=\"mirthTarget\" value=\"${target.id}\"><img src=\"${target.data.img}\"><div>${target.name}</div></label>`);\n            return list;\n        }, []);\n        let content = `\n        <style>\n    #targetList .form-group {\n        display: flex;\n        flex-wrap: wrap;\n        width: 100%;\n        align-items: flex-start;\n      }\n      \n      #targetList .radio-label {\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        text-align: center;\n        justify-items: center;\n        flex: 1 0 25%;\n        line-height: normal;\n      }\n      \n      #targetList .radio-label input {\n        display: none;\n        position:relative;\n      }\n\n      #targetList .radio-label input div {\n       display:block;\n       text-align:center;\n       position: absolute;\n       bottom:5px;\n       left:0;\n       right:0;\n      }\n      \n      #targetList img {\n        border: 0px;\n        width: 50px;\n        height: 50px;\n        flex: 0 0 50px;\n        cursor: pointer;\n      }\n\n      #targetList [type=radio]:hover + img {\n        outline: 2px solid #000;\n      }\n          \n      /* CHECKED STYLES */\n      #targetList [type=radio]:checked + img {\n        outline: 2px solid #f00;\n      }\n    </style>\n    <form id=\"targetList\">        \n    <div class=\"form-group\">Select a Target</div>\n        <div class=\"form-group\">${getTargets.join('')}</div></form>`;\n        new Dialog({\n            title: itemD.name,\n            content,\n            buttons: {\n                use: {\n                    label: `Select`, callback: async (html) => {\n                        let result = html.find(\"input[type='radio'][name='mirthTarget']:checked\");\n                        if (result.length === 0) {\n                            return ui.notifications.warn(`No target!`);\n                        }\n                        resolve(result[0].value);\n                    }\n                },\n                skip: {\n                    label: \"Skip\", callback: () => {\n                        resolve(false);\n                    }\n                }\n            },\n            default: \"Skip\"\n        }).render(true);\n    });\n\n\n\n    let saveType = \"wis\";\n    let target = await canvas.tokens.get(targetFound);\n    let resist = [\"Magic Resistance\"];\n    let getResistance = target.actor.items.find(i => resist.includes(i.name));\n    let immunityList = [\"charmed\"];\n    let immunity = target.actor.data.data.traits.ci.value.find(i => immunityList.includes(i));\n    let getAdvantage = getResistance ? { advantage: true, chatMessage: false, fastForward: true } : { chatMessage: false, fastForward: true };\n    let save = await MidiQOL.socket().executeAsGM(\"rollAbility\", { request: \"save\", targetUuid: target.actor.uuid, ability: saveType, options: getAdvantage });\n    let success = \"saves\";\n    let saveResult;\n    if (immunity) {\n        saveResult = `<div class=\"midi-qol-flex-container\"><div class=\"midi-qol-target-npc midi-qol-target-name\" id=\"${target.id}\">${target.name} is immune</div><div><img src=\"${target.data.img}\" width=\"30\" height=\"30\" style=\"border:0px\"></div></div>`;\n\n    } else {\n        if (save.total < DC) {\n            success = \"fails\";\n            let gameRound = game.combat ? game.combat.round : 0;\n            let condition = game.i18n.localize(\"Charmed\");\n            let conditionIcon = \"icons/svg/heal.svg\";\n            let conditionFlags = { \"dae\": { \"token\": target.actor.uuid, specialDuration: [\"isDamaged\"] } };\n            if ((game.modules.get(\"combat-utility-belt\")) && (game.modules.get(\"combat-utility-belt\")?.active)) {\n                let cubCondition = game.cub?.conditions?.find(i => i.name === condition);\n                if (cubCondition) {\n                    conditionIcon = cubCondition.icon;\n                    conditionFlags[\"combat-utility-belt\"] = { \"conditionId\": cubCondition.id, \"overlay\": false };\n                    conditionFlags[\"core\"] = { \"statusId\": `combat-utility-belt.${cubCondition.id}` };\n                }\n            }\n            let effectData = {\n                label: condition,\n                icon: conditionIcon,\n                origin: lastArg.uuid,\n                disabled: false,\n                flags: conditionFlags,\n                duration: { rounds: 10, startRound: gameRound, startTime: game.time.worldTime },\n                changes: [\n                    { key: \"macro.itemMacro\", mode: CONST.ACTIVE_EFFECT_MODES.CUSTOM, value: `ItemMacro.${itemD.name}`, priority: 20 }\n                ],\n            };\n            let effect = target.actor.effects.find(i => i.data.label === condition);\n            if (!effect) await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: target.actor.uuid, effects: [effectData] });\n        }\n        saveResult = `<div class=\"midi-qol-flex-container\"><div class=\"midi-qol-target-npc midi-qol-target-name\" id=\"${target.id}\">${target.name} ${success} with ${save.total}</div><div><img src=\"${target.data.img}\" width=\"30\" height=\"30\" style=\"border:0px\"></div></div>`;\n    }\n    await wait(600);\n    let save_results = `<div class=\"midi-qol-nobox midi-qol-bigger-text\">${CONFIG.DND5E.abilities[saveType]} Saving Throw: DC ${DC}</div><div><div class=\"midi-qol-nobox\">${saveResult}</div></div>`;\n    let chatMessage = await game.messages.get(lastArg.itemCardId);\n    let content = await duplicate(chatMessage.data.content);\n    let searchString = /<div class=\"midi-qol-hits-display\">[\\s\\S]*<div class=\"end-midi-qol-hits-display\">/g;\n    let replaceString = `<div class=\"midi-qol-hits-display\"><div class=\"end-midi-qol-hits-display\">${save_results}`;\n    content = await content.replace(searchString, replaceString);\n    await chatMessage.update({ content: content });\n    await ui.chat.scrollBottom();\n}\n\nif (args[0] === \"on\") {\n    let target = canvas.tokens.get(lastArg.tokenId);\n    let updates = {\n        token: {\n            \"disposition\": CONST.TOKEN_DISPOSITIONS.FRIENDLY\n        }\n    };\n    await warpgate.mutate(target.document, updates, {}, { name: itemD.name });\n    new Sequence()\n        .effect()\n        .file(\"jb2a.impact_themed.heart.pinkyellow\")\n        .waitUntilFinished(-500)\n        .atLocation(target)\n        .scale(target.data.width / 2)\n    .play()\n}\n\nif (args[0] === \"off\") {\n    let target = canvas.tokens.get(lastArg.tokenId);\n    await warpgate.revert(target.document, itemD.name);\n    ChatMessage.create({\n        content: `${target.name} is no longer charmed.`,\n        speaker: ChatMessage.getSpeaker({ token: target })\n    });\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}},"_id":null,"name":"Mirthful","type":"script","author":"Tyd5yiqWrRZMvG30","img":"icons/svg/dice-target.svg","scope":"global","command":"","folder":null,"sort":0,"permission":{"default":0},"flags":{}}}},"system":{"description":{"value":"<p>The fey can force one creature it can see within 10 feet of it to make a Wisdom saving throw against your spell save DC. Unless the save succeeds, the target is charmed by you and the fey for 1 minute or until the target takes any damage.</p>","chat":"","unidentified":""},"source":"","activation":{"type":"special","cost":null,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"value":null,"long":null,"units":""},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"save","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile":""},"formula":