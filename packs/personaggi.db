ens.get(lastArg.tokenId).actor;\nelse tactor = game.actors.get(lastArg.actorId);\nlet caster;\nif (getProperty(tactor.data.flags, \"warpgate.summonFey.tokenId\")) caster = canvas.tokens.get(getProperty(tactor.data.flags, \"warpgate.summonFey.tokenId\")).actor;\nelse caster = game.actors.get(getProperty(tactor.data.flags, \"warpgate.summonFey.actorId\"));\nconst itemD = lastArg.efData.flags.dae.itemData;\n\nif (args[0] === \"on\") {\n    let effect = await tactor.effects.find(i => (version > 9 ? i.label : i.data.label) === \"Life Monitor\");\n    let effectData = [{\n        origin: lastArg.origin,\n        changes: [\n            { key: \"macro.itemMacro\", mode: CONST.ACTIVE_EFFECT_MODES.CUSTOM, value: `ItemMacro.${itemD.name}`, priority: 20 }\n        ],\n        flags: { dae: { specialDuration: [\"zeroHP\"] } },\n        disabled: false,\n        transfer: false,\n        icon: itemD.img,\n        label: `Life Monitor`\n    }];\n    if (!effect) await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: tactor.uuid, effects: effectData });\n}\n\nif (lastArg[\"expiry-reason\"] === \"midi-qol:zeroHP\") {\n    let findConc = await caster.effects.find(i => (version > 9 ? i.label : i.data.label) === \"Concentrating\");\n    if (findConc) await MidiQOL.socket().executeAsGM(\"removeEffects\", { actorUuid: caster.uuid, effects: [findConc.id] });\n}","folder":null,"sort":0,"permission":{"default":0},"flags":{}}},"core":{"sourceId":"Item.4yhXqPJoxHKsDKHx"}},"system":{"description":{"value":"","chat":"","unidentified":""},"source":"","activation":{"type":"none","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value":null,"long":null,"units":"self"},"uses":{"value":null,"max":"","per":"","recovery":""},"consume":{"type":"","target":"","amount":null},"ability":null,"actionType":"","attackBonus":0,"chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[],"versatile